{
  "name": "@autorest/core",
  "version": "3.0.6371",
  "description": "AutoRest core module",
  "engines": {
    "node": ">=10.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Azure/autorest.git"
  },
  "keywords": [
    "autorest",
    "swagger",
    "openapi"
