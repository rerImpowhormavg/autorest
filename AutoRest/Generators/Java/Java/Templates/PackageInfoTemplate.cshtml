@using System.Linq
@using Microsoft.Rest.Generator.ClientModel
@using Microsoft.Rest.Generator.Java.TemplateModels
@inherits Microsoft.Rest.Generator.Template<Microsoft.Rest.Generator.Java.PackageInfoTemplateModel>
@Header("// ").TrimMultilineHeader()
@EmptyLine

/**
@if (!Model.IsModel)
{
@: * This package contains the classes for @(Model.Title).
}
else
{
@: * This package contains the model classes for @(Model.Title).
}
@if (!string.IsNullOrEmpty(Model.Description))
{
@: * @Model.Description.Period()
}
 */

@if (!Model.IsModel)
{
@:package @(Settings.Namespace.ToLower());
}
else
{
@:package @(Settings.Namespace.ToLower()).models;
}