@using System.Linq
@using Microsoft.Rest.Generator.ClientModel
@using Microsoft.Rest.Generator.Python.TemplateModels
@inherits Microsoft.Rest.Generator.Template<Microsoft.Rest.Generator.Python.EnumTemplateModel>
# coding=utf-8
# --------------------------------------------------------------------------
@Header("# ").TrimMultilineHeader()
# --------------------------------------------------------------------------
@EmptyLine
from enum import Enum
@{
  foreach (var enumType in @Model.EnumTypes)
  {
@EmptyLine
@EmptyLine
@:class @(enumType.Name)(Enum):
@EmptyLine
    for (int i = 0; i < enumType.Values.Count; i++)
    {
@:    @(enumType.Values[i].Name) = "@enumType.Values[i].SerializedName"
    }
  }
}
