{
  "files.exclude": {
    "node_modules/**": true,
    "src/autorest-core/**/*.js": true,
    "src/autorest-core/**/*.d.ts": true,
    "src/autorest-core/**/*.js.map": true,
    "src/autorest/**/*.js": true,
    "src/autorest/**/*.js.map": true,
    "src/autorest/**/*.d.ts": true,
    "src/autorest/node_modules/**": true,
    "src/autorest-core/node_modules/**": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true
  },
  "csharp.suppressDotnetRestoreNotification": true,
  "[csharp]": {
    "editor.tabSize": 4
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.tabSize": 2,
    "editor.detectIndentation": false
  },
  "files.associations": {
    "*.iced": "coffeescript"
  }
}