{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Node.js Attach",
      "port": 9229
    },
    {
      "type": "node",
      "protocol": "inspector",
      "request": "launch",
      "name": "run tests",
      "program": "${workspaceRoot}/src/autorest-core/node_modules/mocha/bin/_mocha",
      "args": [
        "dist/test/profile-filter.js",
        "--timeout=2000000"
      ],
      "cwd": "${workspaceRoot}/src/autorest-core"
    },
    {
      "type": "node",
      "protocol": "inspector",
      "request": "launch",
      "name": "run autorest",
      "program": "${workspaceRoot}/src/autorest/dist/app.js",
      "args": [
        "--version=${workspaceRoot}/src/autorest-core",
        "--verbose",
        "--debug",
        "--output-artifact=specs-used",
        "--interactive"
      ],
      "cwd": "c:/work/2019/azure-powershell/src/network"
    },
    {
      "type": "node",
      "protocol": "inspector",
      "request": "launch",
      "name": "run autorest (loader)",
      "program": "${workspaceRoot}/src/autorest/dist/app.js",
      "args": [
        "--version=${workspaceRoot}/src/autorest-core",
        "--input-file=C:/work/2019/azconfig/azconfig.json ",
        "--output-folder=c:/tmp/output",
        "--use:c:/work/2019/autorest.incubator",
        "--powershell"
      ],
      "cwd": "${workspaceRoot}/Samples/openapi-v2/1c-multiple-input-swaggers"
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach",
      "processId": "16016"
    }
  ]
}