{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Node.js Attach",
      "port": 9229
    },
    {
      "type": "node",
      "protocol": "inspector",
      "request": "launch",
      "name": "run tests",
      "program": "${workspaceRoot}/src/autorest-core/node_modules/mocha/bin/_mocha",
      "args": [
        "dist/test/profile-filter.js",
        "--timeout=2000000"
      ],
      "cwd": "${workspaceRoot}/src/autorest-core"
    },
    {
      "type": "node",
      "protocol": "inspector",
      "request": "launch",
      "name": "run autorest",
      "program": "${workspaceRoot}/src/autorest/dist/app.js",
      "args": [
        "--version=${workspaceRoot}/src/autorest-core",
        "--verbose",
        "--debug",
        "--use=C:/work/2019/autorest.azureresourceschema",
        "--azureresourceschema",
        "C:\\work\\2019\\generated-schemas\\tmp\\azure-rest-api-specs\\specification\\redis\\resource-manager\\readme.md",
        "--output-folder=c:/tmp/schemas",
        "--api-version:2017-10-01"
      ],
      "cwd": "${workspaceRoot}/Samples/openapi-v2/1c-multiple-input-swaggers"
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach",
      "processId": "16016"
    }
  ]
}