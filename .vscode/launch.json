{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Node.js Attach",
      "port": 9229
    },
    {
      "type": "node",
      "protocol": "inspector",
      "request": "launch",
      "name": "run tests",
      "program": "${workspaceRoot}/src/autorest-core/node_modules/mocha/bin/_mocha",
      "args": [
        "dist/test/merger.js",
        "--timeout=2000000"
      ],
      "cwd": "${workspaceRoot}/src/autorest-core"
    },
    {
      "type": "node",
      "protocol": "inspector",
      "request": "launch",
      "name": "run bootstrapper",
      "program": "${workspaceRoot}/src/autorest/dist/app.js",
      "args": [
        "--version=${workspaceRoot}/src/autorest-core",
        "--verbose",
        "--debug",
        "--use:C:/work/2018/autorest.modeler"
      ],
      "cwd": "${workspaceRoot}/Samples/openapi-v2/1c-multiple-input-swaggers"
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:pickProcess}"
    }
  ]
}