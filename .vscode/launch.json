{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Node.js Attach",
      "port": 9229
    },
    {
      "type": "node",
      "protocol": "inspector",
      "request": "launch",
      "name": "run tests",
      "program": "${workspaceRoot}/src/autorest-core/node_modules/mocha/bin/_mocha",
      "args": [
        "dist/test/configuration.js",
        "--timeout=2000000"
      ],
      "cwd": "${workspaceRoot}/src/autorest-core"
    },
    {
      "type": "node",
      "protocol": "inspector",
      "request": "launch",
      "name": "run autorest",
      "program": "${workspaceRoot}/src/autorest/dist/app.js",
      "args": [
        "--version=${workspaceRoot}/src/autorest-core",
        "--verbose",
        "--debug",
        "--use=C:/work/2019/autorest.azureresourceschema",
        "--azureresourceschema",
        "--enable-multi-api" 
        ,"C:/work/2019/generated-schemas/tmp/azure-rest-api-specs/specification/batch/resource-manager/readme.md",
        "--api-version:2017-09-01",
        "--output-folder=c:/tmp/schemas",
        
      ],
      "cwd": "${workspaceRoot}/Samples/openapi-v2/1c-multiple-input-swaggers"
    },
    {
      "type": "node",
      "protocol": "inspector",
      "request": "launch",
      "name": "run autorest (loader)",
      "program": "${workspaceRoot}/src/autorest/dist/app.js",
      "args": [
        "--version=${workspaceRoot}/src/autorest-core",
        "--input-file=C:/work/2019/azconfig/azconfig.json ",
        "--output-folder=c:/tmp/output",
        "--use:c:/work/2019/autorest.incubator",
        "--powershell"
      ],
      "cwd": "${workspaceRoot}/Samples/openapi-v2/1c-multiple-input-swaggers"
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach",
      "processId": "16016"
    }
  ]
}