{
  "version": "1.0.0-*",
  "authors": [ "Microsoft" ],

  "packOptions": {
    "summary": "Autorest Generator Extensions for Azure Tests ",
    "tags": [ "Microsoft AutoRest " ],
    "projectUrl": "https://github.com/Azure/AutoRest",
    "licenseUrl": "https://raw.githubusercontent.com/Microsoft/dotnet/master/LICENSE"
  },
  "testRunner": "xunit",
  "frameworks": {
    "net451": {
    }
  },

  "buildOptions": {
    "delaySign": true,
    "publicSign": false,
    "keyFile": "../../../Tools/MSSharedLibKey.snk",

    "copyToOutput": {
      "include": ["Swagger" ],
      "mappings" : {
        "AutoRest.json" : "../../../src/core/AutoRest/AutoRest.json"
      }
    }
  },

  "dependencies": {
    "xunit": "2.2.0-beta2-build3300",
    "dotnet-test-xunit": "2.2.0-preview2-build1029",
    "Newtonsoft.Json": "[9.0.1,10.0)",

    "AutoRest.Core": {
      "target": "project"
    },
    "AutoRest.Swagger": {
      "target": "project"
    },
    "AutoRest.CSharp.Azure": {
      "target": "project"
    }
  }
}
