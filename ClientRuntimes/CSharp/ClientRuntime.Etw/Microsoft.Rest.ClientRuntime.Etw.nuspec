        <reference file="Microsoft.Rest.ClientRuntime.Etw.dll"/>
      </group>
    </references>
    <dependencies>
      <dependency id="Microsoft.Rest.ClientRuntime" version="__VERSION_Microsoft.Rest.ClientRuntime__"/>
    </dependencies>
  </metadata>
  <files>
    <file src="ClientRuntimes/CSharp/ClientRuntime.Etw/**/*.cs" target="src"/>
    <file src="binaries/net45/Microsoft.Rest.ClientRuntime.Etw.dll" target="lib\net45"/>
    <file src="binaries/net45/Microsoft.Rest.ClientRuntime.Etw.pdb" target="lib\net45"/>
    <file src="binaries/net45/Microsoft.Rest.ClientRuntime.Etw.xml" target="lib\net45"/>
