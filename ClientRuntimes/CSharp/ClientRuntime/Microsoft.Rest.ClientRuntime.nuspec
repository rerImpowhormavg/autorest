<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata minClientVersion="2.5">
    <id>Microsoft.Rest.ClientRuntime</id>
    <title>Client Runtime Library for Microsoft AutoRest Generated Clients</title>
    <version>$version$</version>
    <authors>Microsoft</authors>
    <owners>azure-sdk, Microsoft</owners>
    <licenseUrl>https://raw.githubusercontent.com/Microsoft/dotnet/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/Azure/AutoRest</projectUrl>
    <iconUrl>https://cdn.rawgit.com/Azure/AutoRest/7c1576dfb56974176223545cfac5762d168ded5f/Documentation/images/autorest-small-flat.png</iconUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <summary>Provides HttpClient infrastructure for clients generated by AutoRest.</summary>
    <description>Infrastructure for error handling, tracing, and HttpClient pipeline configuration. Required by client libraries generated using AutoRest.

      Supported Platforms:
      -   Portable Class Libraries
      -   .NET Framework 4.5 
      -   .NET Framework 4.0 
      -   Windows 8
      -   Windows Phone 8.1</description>
    <copyright>Copyright Â© Microsoft Corporation</copyright>
    <tags>Microsoft AutoRest ClientRuntime</tags>
    <references>
      <group targetFramework="portable-net403+win8+wpa81">
        <reference file="Microsoft.Rest.ClientRuntime.dll" />
      </group>
      <group targetFramework="net40">
        <reference file="Microsoft.Rest.ClientRuntime.dll" />
      </group>
      <group targetFramework="net45">
        <reference file="Microsoft.Rest.ClientRuntime.dll" />
      </group>
    </references>
    <dependencies>
      <group targetFramework="portable-net403+win8+wpa81">
        <dependency id="Microsoft.Bcl" version="1.1.9" />
        <dependency id="Microsoft.Bcl.Async" version="1.0.168" />
        <dependency id="Microsoft.Bcl.Build" version="1.0.14" />
        <dependency id="Microsoft.Net.Http" version="2.2.22" />
        <dependency id="Newtonsoft.Json" version="6.0.4" />
      </group>
      <group targetFramework="net40">
        <dependency id="Microsoft.Bcl" version="1.1.9" />
        <dependency id="Microsoft.Bcl.Async" version="1.0.168" />
        <dependency id="Microsoft.Bcl.Build" version="1.0.14" />
        <dependency id="Microsoft.Net.Http" version="2.2.22" />
        <dependency id="Newtonsoft.Json" version="6.0.4" />
      </group>
      <group targetFramework="net45">
        <dependency id="Newtonsoft.Json" version="6.0.4" />
      </group>
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System.Net.Http" targetFramework="net45" />
    </frameworkAssemblies>
  </metadata>
  <files>
    <file src="ClientRuntime\**\*.cs" target="src\Common" />
    <file src="ClientRuntime\Properties\*.resx" target="src\Common\Properties" />
    <file src="..\..\binaries\Portable-Release\Microsoft.Rest.ClientRuntime.dll" target="lib\portable-net403+win8+wpa81" />
    <file src="..\..\binaries\Portable-Release\Microsoft.Rest.ClientRuntime.pdb" target="lib\portable-net403+win8+wpa81" />
    <file src="..\..\binaries\Portable-Release\Microsoft.Rest.ClientRuntime.xml" target="lib\portable-net403+win8+wpa81" />
    <file src="..\..\binaries\Net40-Release\Microsoft.Rest.ClientRuntime.dll" target="lib\net40" />
    <file src="..\..\binaries\Net40-Release\Microsoft.Rest.ClientRuntime.pdb" target="lib\net40" />
    <file src="..\..\binaries\Net40-Release\Microsoft.Rest.ClientRuntime.xml" target="lib\net40" />
    <file src="..\..\binaries\Net45-Release\Microsoft.Rest.ClientRuntime.dll" target="lib\net45" />
    <file src="..\..\binaries\Net45-Release\Microsoft.Rest.ClientRuntime.pdb" target="lib\net45" />
    <file src="..\..\binaries\Net45-Release\Microsoft.Rest.ClientRuntime.xml" target="lib\net45" />
  </files>
</package>
