{
  "name": "@autorest/core",
  "version": "3.0.0",
  "patchOffset": 5000,
  "description": "AutoRest core module",
  "engines": {
    "node": ">=10.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Azure/autorest.git"
  },
  "keywords": [
    "autorest",
    "swagger",
    "openapi"
  ],
  "author": "Microsoft Corporation",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Azure/autorest/issues"
  },
  "homepage": "https://github.com/Azure/autorest#readme",
  "readme": "https://github.com/Azure/autorest/blob/master/readme.md",
  "main": "./dist/exports.js",
  "bin": {
    "autorest-core": "./entrypoints/app.js",
    "autorest-language-service": "./entrypoints/language-service.js"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha ./dist/test --timeout 100000",
    "watch": "tsc -p . --watch",
    "build": "npm run before-build && tsc -p .",
    "static-link": "static-link --no-node-modules --debug",
    "eslint-fix": "eslint  . --fix --ext .ts",
    "eslint": "eslint  . --ext .ts",
    "prepack": "npm run static-link && npm run build",
    "run-pwsh": "pwsh -noprofile -command \"& {param($script) pwsh -noprofile -command ((convertfrom-json (get-content -raw ./package.json)).'pwsh-scripts'.$script) }\"",
    "clean": "pwsh -noprofile -command \"remove-item -recurse -ea 0 ./dist ; remove-item -ea 0 ./*.log\"",
    "before-build": "pwsh -noprofile -command \"new-item -itemType directory -ea 0 -path dist/lib/pipeline ; copy-item ../schema/*.json dist/lib/pipeline\""
  },
  "typings": "./dist/main.d.ts",
  "devDependencies": {
    "@types/commonmark": "^0.27.0",
    "@types/jsonpath": "^0.2.0",
    "@types/source-map": "0.5.0",
    "@types/z-schema": "^3.16.31",
    "@types/mocha": "5.2.5",
    "mocha-typescript": "1.1.17",
    "deep-equal": "^1.0.1",
    "@types/node": "12.7.2",
    "mocha": "5.2.0",
    "typescript": "~3.6.2",
    "@typescript-eslint/eslint-plugin": "~2.0.0",
    "@typescript-eslint/parser": "~2.0.0",
    "eslint": "~6.2.2",
    "static-link": "^0.2.4",
    "vscode-languageserver": "3.5.1",
    "@azure-tools/async-io": "~3.0.0",
    "@azure-tools/extension": "~3.0.0",
    "@azure-tools/uri": "~3.0.0",
    "@azure-tools/datastore": "~4.0.0",
    "@azure-tools/oai2-to-oai3": "~4.0.0",
    "@azure-tools/codegen": "~2.1.0",
    "@azure-tools/openapi": "~3.0.0",
    "@azure-tools/linq": "~3.1.0",
    "@azure-tools/tasks": "~3.0.0",
    "@azure-tools/deduplication": "~3.0.0",
    "@azure-tools/object-comparison": "~3.0.0",
    "compare-versions": "^3.4.0",
    "commonmark": "^0.27.0",
    "jsonpath": "1.0.0",
    "linq-es2015": "^2.4.25",
    "safe-eval": "^0.3.0",
    "source-map": "0.5.6",
    "untildify": "^3.0.2",
    "vscode-jsonrpc": "^3.5.0",
    "source-map-support": "0.5.13",
    "yaml-ast-parser": "0.0.40",
    "pwsh": "0.2.0",
    "z-schema": "^3.19.0"
  },
  "static-link": {
    "entrypoints": [],
    "dependencies": {
      "vscode-languageserver": "3.5.1",
      "@azure-tools/async-io": "~3.0.0",
      "@azure-tools/extension": "~3.0.0",
      "@azure-tools/uri": "~3.0.0",
      "@azure-tools/datastore": "~4.0.0",
      "@azure-tools/oai2-to-oai3": "~4.0.0",
      "@azure-tools/codegen": "~2.1.0",
      "@azure-tools/openapi": "~3.0.0",
      "@azure-tools/linq": "~3.1.0",
      "@azure-tools/tasks": "~3.0.0",
      "@azure-tools/deduplication": "~3.0.0",
      "@azure-tools/object-comparison": "~3.0.0",
      "commonmark": "^0.27.0",
      "jsonpath": "1.0.0",
      "compare-versions": "^3.4.0",
      "linq-es2015": "^2.4.25",
      "safe-eval": "^0.3.0",
      "source-map": "0.5.6",
      "untildify": "^3.0.2",
      "vscode-jsonrpc": "^3.5.0",
      "yaml-ast-parser": "0.0.40",
      "z-schema": "^3.19.0"
    }
  },
  "dependencies": {}
}